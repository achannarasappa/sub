{
  "name": "subjs",
  "version": "1.0.0",
  "description": "",
  "main": "lib/cli.js",
  "bin": {
    "sub": "lib/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix . || exit 0",
    "test": "mkdir -p tmp && ./node_modules/.bin/ava --serial test/",
    "build": "./node_modules/.bin/pkg package.json -o sub -t=node8-linux-x64,node8-macos-x64 && mkdir -p build && mv ./sub-* build/",
    "coverage": "./node_modules/.bin/nyc npm test",
    "coverage:coveralls": "npm run coverage && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Ani Channarasappa <npm@fireantx.com> (https://github.com/achannarasappa)",
  "license": "MIT",
  "devDependencies": {
    "@achannarasappa/eslint-config": "^1.0.1",
    "ava": "^0.24.0",
    "colors": "^1.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.1",
    "intercept-stdout": "^0.1.2",
    "nyc": "^11.3.0",
    "pkg": "^4.3.0-beta.1",
    "shelljs": "^0.7.8"
  },
  "dependencies": {
    "fs-extra": "^4.0.3",
    "get-stdin": "^5.0.1",
    "glob": "^7.1.2",
    "highland": "^2.11.1",
    "prettyjson": "^1.2.1",
    "ramda": "^0.25.0",
    "tmp": "0.0.33",
    "yargs": "^10.0.3"
  },
  "eslintConfig": {
    "extends": "@achannarasappa/eslint-config"
  },
  "eslintIgnore": [
    "lib/cli.js"
  ]
}
